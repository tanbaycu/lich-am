{
  "manifest_version": 3,
  "name": "Lịch âm",
  "version": "1.0.0",
  "description": "Không gian lịch âm, thời tiết và widget sáng tạo cho người Việt.",
  "permissions": ["storage", "geolocation"],
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "action": {
    "default_icon": "assets/icon.png"
  },
  "icons": {
    "16": "assets/icon.png",
    "48": "assets/icon.png",
    "128": "assets/icon.png"
  }
}
